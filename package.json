{
  "name": "comet-auto-completer",
  "displayName": "Comet Auto Completer",
  "description": "This is the auto completer of comet(datapack compiler) by 40planet\nAuthor : Jungha",
  "author": {
    "name": "Jungha"
  },
  "version": "0.0.1",
  "publisher": "CometAutoCompleter",
  "repository": {
    "type": "git",
    "url": "https://github.com/jungha0110/comet-auto-completer"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:planet"
  ],
  "contributes": {
    "languages": [
      {
        "id": "planet",
        "extensions": [".planet"],
        "aliases": ["Planet", "planet"]
      }
    ]
  },
  "main": "./extension.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
